{
  "name": "katowice2019-elo-calc",
  "version": "1.1.4",
  "homepage": "./",
  "description": "Katowice 2019 Elo Calculator",
  "repository": {
    "type": "git",
    "url": "git://github.com/react-boilerplate/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=5",
    "node": ">=8.10.0"
  },
  "author": "ieb",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && cp -rf src/images build/ && cp _redirects build/",
    "test": "react-scripts test",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "prettify": "prettier --write",
    "publish:github": "cross-env PUBLIC_URL='/react-boilerplate-cra-template' yarn build && gh-pages -d build",
    "changelog": "ts-node --project=./internals/ts-node.tsconfig.json ./internals/scripts/create-changelog.script.ts"
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:eslint:fix",
      "git add --force"
    ],
    "*.json": [
      "prettier --write",
      "git add --force"
    ]
  },
  "pre-commit": "lint:staged",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0",
    "react-error-overlay": "6.0.9"
  },
  "dependencies": {
    "@artsy/fresnel": "^6.1.0",
    "d3": "7.8.2",
    "helmet": "4.6.0",
    "kofi-button": "1.1.1",
    "mathjs": "~11.7.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-helmet": "~6.1.0",
    "react-redux": "~7.2.5",
    "react-router-dom": "~5.3.0",
    "sanitize.css": "~13.0.0",
    "semantic-ui-css": "^2.5.0",
    "semantic-ui-react": "^2.1.2"
  },
  "devDependencies": {
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "3.4.0",
    "prettier": "2.3.2",
    "react-scripts": "^4.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
